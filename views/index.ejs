							<ul class="drop">
								<span data-ng-repeat="sublists in brand">
								<span data-ng-if="lists._id==sublists.category">
									<li><a ui-sref="/category({category:lists.url,brand:sublists.url,catId:lists._id,brandId:sublists._id})">{{sublists.name}}</a></li>
								</span>
								</span>
							</ul>
					</li>
					
				</ul>
				
			</div>
		</div>
